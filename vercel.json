{
  "buildCommand": "npm run build",
  "outputDirectory": "frontend/dist",
  "crons": [
    {
      "path": "/api/cron/finalize-accesses",
      "schedule": "0 * * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
